{
  "name": "react-marked-preview-editor",
  "version": "1.1.0",
  "description": "A markdown editor and/or preview",
  "main": "./lib/index.js",
  "scripts": {
    "prepublish": "npm run babel && cp src/style/font-awosome.css lib/style/",
    "test": "./node_modules/jest-cli/bin/jest.js --coverage",
    "babel": "./node_modules/babel-cli/bin/babel.js src -d lib",
    "example": "npm run prepublish && webpack-dev-server --config ./example/webpack.config.js --inline --color --progress"
  },
  "keywords": [
    "react-component",
    "markdown",
    "react",
    "editor",
    "preview"
  ],
  "author": "Yinjie <yj1438@163.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yj1438/react-marked-preview-editor/issues",
    "email": "yj1438@163.com"
  },
  "jest": {
    "transform": {
      ".*": "./scripts/preprocessor.js"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/reflux",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/react-tools",
      "<rootDir>/node_modules/enzyme"
    ],
    "collectCoverage": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yj1438/react-marked-preview-editor"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "reflux": "^0.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "flowchart.js": "^1.6.6",
    "github-markdown-css": "^2.8.0",
    "html-webpack-plugin": "^2.29.0",
    "raphael": "^2.2.7",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^1.15.0"
  }
}
